<body id = "fondo_habitaciones">
<div id = "contener_habitaciones">
  <% if user_signed_in? && current_user.user? %>
    <h1><%= @reservas.id %></h1>
    <div>
      <%= @reservas.habitacions_id %>
    </div>
    <div>
      <%= @reservas.fecha_ingreso %>
    </div>
    <div>
      <%= @reservas.fecha_salida %>
    </div>
    <div class = "btn btn-info" id = "boton_guardado_error">
      <%= link_to "Editar", edit_reserva_path(@reservas) %>
    </div>
    <div class = "btn btn-danger" id = "boton_guardado_error">
      <%= link_to "Eliminar", {:action => :destroy, :id => @reservas.id}, :method => :delete, data: { :confirm => "Esta seguro que deesea eliminar la reserva"} %>
    </div>
  <%end%>
</div>
</body>